<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%- include("../views/partials/diseno/cabecera") %>
  <title>Document</title>
</head>
<body>

  <div class="d-flex justify-content-center">
    
    
    <div class="col-3 izquierda"><%- include("../views/partials/diseno/margenIzquierda") %></div>
    
    <div class="col-6 actualizar">

      <form action="/actualizar/<%= suscriptorActualizado._id %>" method="POST">
        <div class="mb-3">
          <label for="" class="form-label">Nombre</label>
          <input
            type="text"
            name="nombre"
            class="form-control"
            aria-describedby="emailHelp"
            value="<%= suscriptorActualizado.nombre %>"
          />
      
          <label for="" class="form-label">Correo electronico</label>
          <input
            type="email"
            name="correo"
            class="form-control"
            aria-describedby="emailHelp"
            value="<%= suscriptorActualizado.correo %>"
          />
      
          <div class="text-center">
          <button type="submit" class="btnActualizar btn btn-success text-center">ACTUALIZAR</button></div>
        </div>
      </form>

    </div>
    <div class="col-3 derecha">
      <%- include("../views/partials/diseno/margenDerecha") %>
    </div>

    
  </div>
  <%- include("../views/partials/diseno/cuerpo") %>
</body>
</html>



